  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@types/geojson": "^7946.0.7",
    "babel-plugin-macros": "^2.8.0",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-device-detect": "^1.12.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.10.0",
    "react-leaflet": "^2.6.3",
    "react-papaparse": "^3.0.3",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.1",
    "superagent": "^5.2.2",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extract": "formatjs extract 'src/**/*.ts*' --out-file ./src/i18n/extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile:en": "formatjs compile ./src/i18n/extracted/en.json --ast --out-file ./src/i18n/locales/en.json",
    "compile:es": "formatjs compile ./src/i18n/extracted/es.json --ast --out-file ./src/i18n/locales/es.json",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^2.13.11",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-leaflet": "^2.5.2",
    "@types/react-redux": "^7.1.9",
    "babel-plugin-react-intl": "^8.2.16",
    "gh-pages": "^2.2.0",
    "prettier": "^2.1.1",
    "react-intl-cra": "^0.3.4"
  }
}
